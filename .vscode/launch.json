{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Attach to RAS",
            "type": "python",
            "request": "attach",
            "pathMappings": [
                {
                    "localRoot": "${workspaceRoot}",
                    "remoteRoot": "/home/pi/ras"
